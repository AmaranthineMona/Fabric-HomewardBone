{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Homeward Bone",
  "description": "Allows the use of a homeward bone consumable to teleport to your attuned home anchor",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/AmaranthineMona/Fabric-HomewardBone"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.amar.Homeward"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {
    "cardinal-components": [
      "amar:homeward"
    ]
  }
}
